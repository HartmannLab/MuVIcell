   "source": [
    "## 7. Advanced Statistical Analysis\n",
    "\n",
    "Let's perform some statistical tests to identify significant differences:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from scipy import stats\n",
    "from scipy.stats import f_oneway\n",
    "\n",
    "# Perform ANOVA for each gene across cell types\n",
    "anova_results = []\n",
    "\n",
    "for gene in gene_cols:\n",
    "    groups = [processed_data[processed_data['cell_type'] == ct][gene].values \n",
    "              for ct in processed_data['cell_type'].unique()]\n",
    "    \n",
    "    f_stat, p_value = f_oneway(*groups)\n",
    "    anova_results.append({\n",
    "        'gene': gene,\n",
    "        'f_statistic': f_stat,\n",
    "        'p_value': p_value\n",
    "    })\n",
    "\n",
    "anova_df = pd.DataFrame(anova_results)\n",
    "anova_df['p_adjusted'] = stats.false_discovery_rate(anova_df['p_value'])[1]\n",
    "anova_df = anova_df.sort_values('p_adjusted')\n",
    "\n",
    "print(\"Top 10 most significantly different genes (ANOVA):\")\n",
    "print(anova_df.head(10)[['gene', 'f_statistic', 'p_adjusted']].round(6))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 8. Custom Analysis Pipeline Class\n",
    "\n",
    "Let's create a custom analysis class that extends MuVIcell functionality:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class AdvancedMuVIcellAnalyzer(MuVIcellAnalyzer):\n",
    "    \"\"\"Extended analyzer with additional functionality.\"\"\"\n",
    "    \n",
    "    def __init__(self, data=None):\n",
    "        super().__init__(data)\n",
    "        self.dimensionality_reduction = {}\n",
    "        self.clustering_results = {}\n",
    "        \n",
    "    def perform_pca(self, feature_cols, n_components=2):\n",
    "        \"\"\"Perform PCA analysis.\"\"\"\n",
    "        X = self.data[feature_cols].values\n",
    "        pca = PCA(n_components=n_components)\n",
    "        X_pca = pca.fit_transform(X)\n",
    "        \n",
    "        self.dimensionality_reduction['pca'] = {\n",
    "            'model': pca,\n",
    "            'transformed_data': X_pca,\n",
    "            'explained_variance_ratio': pca.explained_variance_ratio_\n",
    "        }\n",
    "        \n",
    "        # Add to dataframe\n",
    "        for i in range(n_components):\n",
    "            self.data[f'PC{i+1}'] = X_pca[:, i]\n",
    "        \n",
    "        return X_pca\n",
    "    \n",
    "    def perform_clustering(self, feature_cols, n_clusters=5, method='kmeans'):\n",
    "        \"\"\"Perform clustering analysis.\"\"\"\n",
    "        X = self.data[feature_cols].values\n",
    "        \n",
    "        if method == 'kmeans':\n",
    "            model = KMeans(n_clusters=n_clusters, random_state=42)\n",
    "        else:\n",
    "            raise ValueError(f\"Unsupported clustering method: {method}\")\n",
    "        \n",
    "        labels = model.fit_predict(X)\n",
    "        \n",
    "        self.clustering_results[method] = {\n",
    "            'model': model,\n",
    "            'labels': labels,\n",
    "            'n_clusters': n_clusters\n",
    "        }\n",
    "        \n",
    "        self.data['cluster'] = labels\n",
    "        return labels\n",
    "    \n",
    "    def get_cluster_markers(self, feature_cols, cluster_col='cluster'):\n",
    "        \"\"\"Find marker genes for each cluster.\"\"\"\n",
    "        markers = {}\n",
    "        \n",
    "        for cluster_id in self.data[cluster_col].unique():\n",
    "            cluster_data = self.data[self.data[cluster_col] == cluster_id]\n",
    "            other_data = self.data[self.data[cluster_col] != cluster_id]\n",
    "            \n",
    "            cluster_means = cluster_data[feature_cols].mean()\n",
    "            other_means = other_data[feature_cols].mean()\n",
    "            \n",
    "            fold_changes = cluster_means / (other_means + 1e-8)\n",
    "            markers[cluster_id] = fold_changes.sort_values(ascending=False).head(5)\n",
    "        \n",
    "        return markers\n",
    "\n",
    "# Initialize advanced analyzer\n",
    "advanced_analyzer = AdvancedMuVIcellAnalyzer(data=processed_data)\n",
    "\n",
    "# Perform PCA\n",
    "pca_result = advanced_analyzer.perform_pca(gene_cols, n_components=3)\n",
    "print(f\"PCA completed. Explained variance: {advanced_analyzer.dimensionality_reduction['pca']['explained_variance_ratio']}\")\n",
    "\n",
    "# Perform clustering\n",
    "cluster_labels = advanced_analyzer.perform_clustering(gene_cols, n_clusters=6)\n",
    "print(f\"Clustering completed. Found {len(np.unique(cluster_labels))} clusters\")\n",
    "\n",
    "# Get cluster markers\n",
    "markers = advanced_analyzer.get_cluster_markers(gene_cols)\n",
    "print(\"\\nTop marker genes for each cluster:\")\n",
    "for cluster_id, cluster_markers in markers.items():\n",
    "    print(f\"Cluster {cluster_id}: {', '.join(cluster_markers.head(3).index)}\")"
   ]
